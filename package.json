{
  "name": "hempin-ui",
  "version": "0.1.0",
  "private": true,
  "description": "Hempin UI components and shell",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup && npm run play:build",
    "dev": "npm-run-all -p dev:watch-lib dev:playground",
    "dev:watch-lib": "tsup --watch",
    "dev:playground": "cd playground && cross-env NEXT_TELEMETRY_DISABLED=1 next dev",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist playground/.next playground/node_modules",
    "play:build": "cd playground && npm install && cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "play:start": "cd playground && cross-env NEXT_TELEMETRY_DISABLED=1 next start -p 3000"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "rimraf": "^5.0.5",
    "tsup": "^8.5.0",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "clsx": "^2.1.1"
  },
  "engines": {
    "node": "20.x"
  }
}